    <script>
        // Load Screen
        window.addEventListener('load', () => {
            // Hide the loading div when the page has finished loading
            document.querySelector('.loading').style.display = 'none';
        });

        // Dashboard resizing and hide
        const dashboardDiv = document.querySelector('.dashboard-nav-div');
        const dashboardNavbar = document.querySelector('.dashboard-navbar');
        const computedStyle = window.getComputedStyle(dashboardNavbar);
        const minWidth = computedStyle.getPropertyValue("min-width");
        const resizeHandle = document.querySelector('.resize-handle');

        let isDragging = false;
        let currentWidth = dashboardNavbar.offsetWidth;
        let previousX = 0;
        resizeHandle.addEventListener('mousedown', (e) => {
            isDragging = true;
            resizeHandle.classList.add('dragging');
        })

        document.addEventListener('mousemove', (e) => {
            if(isDragging) {
                currentWidth = e.clientX;
                dashboardNavbar.style.width = `${currentWidth}px`;
                if (e.clientX < parseInt(minWidth) - 125) {
                    dashboardDiv.classList.add('shift-left');
                } else if (e.clientX > previousX) {
                    dashboardDiv.classList.remove('shift-left');
                }
                previousX = e.clientX;
            }
        });

        document.addEventListener('mouseup', (e) => {
            isDragging = false;
            resizeHandle.classList.remove('dragging');
        });

        resizeHandle.addEventListener('click', () => {
            dashboardDiv.class.add('shift-left')
        })

        // Account Dropdown Menus
        const accountInfoBtn = document.querySelector('.account-info-btn');
        const dashboardDropdown = document.querySelector('.dashboard-dropdown');
        const accountInfoBtnText = document.querySelector('.text-left');
        accountInfoBtn.addEventListener('click', () => toggleDropdown(dashboardDropdown));
        accountInfoBtnText.addEventListener('click', () => toggleDropdown(dashboardDropdown));

        const profileImg = document.querySelector('.profile-img');
        const accountDropdown = document.querySelector('.account-dropdown');
        profileImg.addEventListener('click', () => {
            toggleDropdown(accountDropdown);
            const rect = profileImg.getBoundingClientRect();
            accountDropdown.style.transform = `translate3D(${rect.left}px, ${rect.bottom + 5}px, 0)`;
        });

        const businessInfoBtn = document.querySelector('.business-info-btn');
        const businessInfoDropdown = document.querySelector('.business-info-dropdown');
        businessInfoBtn.addEventListener('click', () => {
            toggleDropdown(businessInfoDropdown);
            const rect = businessInfoBtn.getBoundingClientRect();
            businessInfoDropdown.style.transform = `translate3D(${rect.left}px, ${rect.bottom + 5}px, 0)`;
        });

        function toggleDropdown(dropdown) {
            event.stopPropagation();
            dropdown.classList.toggle('show');
        }

        window.addEventListener('click', (e) => {
            if (!e.target.closest('.dashboard-dropdown') && e.target !== accountInfoBtn && 
                !e.target.closest('.account-dashboard') && e.target !== profileImg &&
                !e.target.closest('.business-info-dropdown') && e.target !== businessInfoBtn) {
                if (dashboardDropdown) {
                    dashboardDropdown.classList.remove('show');
                }
                if (accountDropdown) {
                    accountDropdown.classList.remove('show');
                }
                if (businessInfoDropdown) {
                    businessInfoDropdown.classList.remove('show');
                }
            }
        });

        // Tools Dropdown Menus
        const toolDivs = document.querySelectorAll('.tool-div');

        function toggleSubTools(subToolsDiv) {
            @* subToolsDiv.style.setProperty('--sub-tools-height', `${subToolsDiv.getBoundingClientRect().height}px`); *@
            subToolsDiv.classList.toggle('open');
        }

        toolDivs.forEach(toolDiv => {
            toolDiv.addEventListener('click', event => {
                const subTools = toolDiv.nextElementSibling;
                toggleSubTools(subTools);
            });
        });

        const subToolsDivs = document.querySelectorAll('.sub-tools');
        subToolsDivs.forEach(subToolsDiv => {
            subToolsDiv.addEventListener('click', event => {
                const nestedSubTools = subToolsDiv.querySelectorAll('.sub-tools');
                nestedSubTools.forEach(nestedSubTool => {
                    nestedSubTool.addEventListener('click', () => {
                        toggleSubTools(nestedSubTool)
                    });
                });
            });
        });

        @* 
        const toolsDropdowns = document.querySelectorAll('.tools-dropdown');

        function toggleToolDiv(toolDiv) {
            toolDiv.classList.toggle('close');
        }

        toolsDropdowns.forEach(toolDropdown => {
            toolDropdown.addEventListener('click', event => {
                const toolDiv = toolDropdown.nextElementSibling;
                toggleToolDiv(toolDiv);
            });
        });         
        *@
    </script>
